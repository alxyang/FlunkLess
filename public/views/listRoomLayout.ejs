 <a href="#" ng-click="grid = !grid"> Switch to Grid View </a>
<div class="grid">
	<div class="row">
			<%- include roomcreation %>

		<!-- <div class="span8" style="height:500px; overflow:visible"> -->
    <div>
      <div class="table-responsive">
      <table class="table table-hover">
        <thead>
          <tr>
            <th>#</th>
          </tr>
        </thead>
        <tbody>
        <tr ng-repeat="room in rooms | filter : {name : roomname} | filter: addedInRoom" ng-cloak>
      		<td class="table-list" ng-click='addRoom(room)'>
          			{{ room.name }}
          </td>
        </tbody>
      </table>
      </div>
		</div>
	</div>
</div>